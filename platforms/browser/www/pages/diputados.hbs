<ons-list >
	{{#each lista}}
	<ons-list-item modifier="chevron material" onclick="LoadProfile({{id}})" tappable>
		<div class="left"><img class="list-item__thumbnail border-{{blankid partido}}" src="{{img.mini.url}}" /></div>
		<div class="center"><span class="list-item__title">{{nombre}} {{apellido}}</span>
		<span class="list-item__subtitle"><b>Distrito: {{distrito}}</b></span></div>
	</ons-list-item>
	{{/each}}
</ons-list>


