<ons-if platform="android">
<iframe frameborder="0" allowfullscreen="true" src="https://www.youtube.com/embed/live_stream?channel=UC_3biGcNq8hEPq_n8zG2OKg&amp;enablejsapi=1&amp;amp=1&amp;playsinline=1&amp;iv_load_policy=3" class="i-amphtml-fill-content" allow="autoplay;" style="width: 100%;"></iframe>
{{#if nextorder.fecha}}
<center><p>Proxima en {{nextorder.fecha}}</p></cetner>
{{/if}}
<ons-list>
{{#each ordenes_anteriores}}
<ons-list-item modifier="longdivider chevron material" onclick="cordova.InAppBrowser.open('{{video}}', '_system'); return false;" rel="external" target="_blank" tappable><div class="left"><div class="gold-circle">{{inc @index}}</div></div> <div class="center"> Tansmisión del día {{fecha}}</div>
</ons-list-item>
{{/each}}
</ons-list>
</ons-if>
<ons-if platform="ios">
{{#if nextorder.fecha}}
<center><p>Proxima en {{nextorder.fecha}}</p></cetner>
{{/if}}
<ons-list>
{{#each ordenes_anteriores}}
<center>
<a onclick="cordova.InAppBrowser.open('https://www.youtube.com/embed/live_stream?channel=UC_3biGcNq8hEPq_n8zG2OKg&amp;enablejsapi=1&amp;amp=1&amp;playsinline=1&amp;iv_load_policy=3', '_system'); return false;"> Abrir Canal en Vivo<img src="data:image/svg+xml;base64," width="40px;"></a>
</center>
<ons-list-item modifier="longdivider chevron material" onclick="cordova.InAppBrowser.open('{{video}}', '_system'); return false;" rel="external" target="_blank" tappable><div class="left"><div class="gold-circle">{{inc @index}}</div></div> <div class="center"> Tansmisión del día {{fecha}}</div>
</ons-list-item>
{{/each}}
</ons-if>